{
  "name": "vimspired",
  "displayName": "Vimspired",
  "description": "Create your own Vim-inspired keybindings!",
  "version": "6.4.4",
  "publisher": "bmalehorn",
  "engines": {
    "vscode": "^1.59.0"
  },
  "repository": {
    "url": "https://github.com/bmalehorn/vscode-vimspired"
  },
  "categories": [
    "Keymaps",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "main": "./dist/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Vimspired",
      "properties": {
        "vimspired.keybindings": {
          "type": "object",
          "description": "Keybindings map, key â†’ vscode command",
          "default": {
            "i": "vimspired.toggle"
          }
        },
        "vimspired.insertCursorStyle": {
          "type": "string",
          "enum": [
            "block",
            "block-outline",
            "line",
            "line-thin",
            "underline",
            "underline-thin"
          ],
          "default": "line",
          "description": "Shape of the cursor when in insert mode."
        },
        "vimspired.normalCursorStyle": {
          "type": "string",
          "enum": [
            "block",
            "block-outline",
            "line",
            "line-thin",
            "underline",
            "underline-thin"
          ],
          "default": "block",
          "description": "Shape of the cursor when in normal mode."
        }
      }
    },
    "commands": [
      {
        "command": "vimspired.toggle",
        "title": "Vimspired: toggle normal / insert mode"
      },
      {
        "command": "vimspired.enterNormal",
        "title": "Vimspired: normal mode"
      },
      {
        "command": "vimspired.enterInsert",
        "title": "Vimspired: insert mode"
      }
    ],
    "keybindings": [
      {
        "key": "Escape",
        "command": "vimspired.enterNormal",
        "when": "editorTextFocus"
      }
    ]
  },
  "icon": "images/vimspired.png",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "@types/vscode": "^1.59.0",
    "eslint": "^8.38.0",
    "glob": "^10.0.0",
    "husky": "^2.4.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.7",
    "pretty-quick": "^1.11.0",
    "ts-loader": "^6.0.2",
    "typescript": "^5.0.4",
    "vscode-test": "^1.6.1",
    "@vscode/test-electron": "^2.3.0",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  },
  "__metadata": {
    "id": "20b6ea6f-43c5-4004-b393-c237975cbd76",
    "publisherDisplayName": "Brian Malehorn",
    "publisherId": "1215635c-400c-4cc8-96d3-f2d5da4774a4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
